{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EPPlus": {
    "ExcelPackage": {
      "LicenseContext": "NonCommercial" //The license context used
    }
  },
  "ConnectionStrings": {
    "Default": "Data Source=HPPROBOOK;Database=BusinessTrackDb;Integrated Security=True"
  },
  "IdentitySettings": {
    "RequireDigit": false,
    "RequireUppercase": false,
    "RequiredLength": 1,
    "RequireLowercase": false,
    "RequireNonAlphanumeric": false
  },
  "CookieSettings": {
    "Name": "JobTrackingSystemCache",
    "HttpOnly": true,
    "ExpireTimeSpanMinute": 30
  },
  "AllowedHosts": "*"
}
